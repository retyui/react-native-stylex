{
  "name": "react-native-stylex",
  "version": "4.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "git@github.com:retyui/react-native-stylex.git",
  "author": "David <4661784+retyui@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "tsc",
    "postbuild": "mkdir lib/makeUseStyles && cp src/makeUseStyles/* lib/makeUseStyles && rm -rf lib/**/*test.* && prettier --write lib/*.js lib/*.d.ts",
    "clean-publish": "yarn test && yarn build && npx clear-package-json package.json > lib/package.json && cp CHANGELOG.md LICENSE README.md lib",
    "publish-next": "yarn clean-publish && cd lib && npm publish --tag next",
    "presize": "yarn build",
    "lint": "eslint './src/*.{js,ts,tsx}' --quiet",
    "test": "jest --coverage",
    "size": "size-limit",
    "ci": "yarn lint && yarn size"
  },
  "peerDependencies": {
    "react": ">=16.4.0",
    "react-native": ">=0.59.0"
  },
  "devDependencies": {
    "@size-limit/preset-app": "^4.9.2",
    "@testing-library/react-hooks": "^5.0.3",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.0",
    "@types/react-native": "^0.63.46",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "clean-publish": "^1.1.8",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-native": "^0.63.4",
    "react-native-dark-mode": "^0.2.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-test-renderer": "^17.0.1",
    "size-limit": "^4.9.2",
    "typescript": "^4.1.3"
  }
}
