{
  "name": "react-native-stylex",
  "version": "3.0.0-beta.0",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "git@github.com:retyui/react-native-stylex.git",
  "author": "David <4661784+retyui@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "tsc --outDir lib",
    "postbuild": "rm -rf lib/__tests__ && cp src/*.d.ts lib/ && prettier --write lib/*.js lib/*.d.ts",
    "bundleLib": "yarn test && yarn build && npx clear-package-json package.json > lib/package.json && cp CHANGELOG.md LICENSE README.md lib",
    "lint": "eslint './src/*.{js,ts,tsx}' --quiet",
    "test": "jest --coverage"
  },
  "peerDependencies": {
    "react": ">=16.4.0",
    "react-native": ">=0.59.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.35",
    "@types/react-native": "^0.62.10",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "clean-publish": "^1.1.7",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "react": "^16.9.0",
    "react-native": "^0.62.2",
    "react-native-appearance": "^0.3.4",
    "react-native-dark-mode": "^0.2.2",
    "react-native-safe-area-context": "^1.0.2",
    "typescript": "^3.9.3"
  }
}
