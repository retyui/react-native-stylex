{
  "name": "react-native-stylex",
  "version": "2.2.0",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "git@github.com:retyui/react-native-stylex.git",
  "author": "David <4661784+retyui@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf lib/*",
    "build": "yarn tsc",
    "postbuild": "rm -rf lib/__tests__ && cp src/*.d.ts lib/ && prettier --write lib/*.js lib/*.d.ts",
    "bundleLib": "yarn test && yarn build && npx clear-package-json package.json > lib/package.json && cp CHANGELOG.md LICENSE README.md lib",
    "lint": "eslint './src/*.{js,ts,tsx}' --quiet",
    "test": "jest --coverage"
  },
  "peerDependencies": {
    "react": ">=16.4.0",
    "react-native": ">=0.59.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.23",
    "@types/react-native": "^0.61.22",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "clean-publish": "^1.1.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-native": "^0.61.5",
    "react-native-appearance": "^0.3.3",
    "react-native-dark-mode": "^0.2.2",
    "react-native-safe-area-context": "^0.7.3",
    "typescript": "^3.8.3"
  }
}
